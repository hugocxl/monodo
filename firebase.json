{
  "hosting": [
    {
      "target": "frontend",
      "public": "frontend/dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {
    "source": "backend/isolate",
    "runtime": "nodejs18",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
