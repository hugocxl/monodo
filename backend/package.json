  "dependencies": {
    "chalk": "^5",
    "compression": "^1",
    "cors": "^2",
    "express": "next",
    "express-mongo-sanitize": "^2",
    "helmet": "^7",
    "http-status": "^1",
    "joi": "^17",
    "mongoose": "^8",
    "morgan": "^1.10.0",
    "passport": "^0",
    "passport-jwt": "^4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2",
    "@types/compression": "^1",
    "@types/cookie-parser": "^1",
    "@types/cors": "2",
    "@types/express": "^4",
    "@types/morgan": "^1",
    "@types/passport": "1"
  }
}
