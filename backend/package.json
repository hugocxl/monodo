{
  "name": "@monodo/backend",
  "description": "Backend for Monodo",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "require": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Hugo Corta <corta.hugo@gmail.com>",
  "scripts": {
    "dev": "tsup --format=esm --no-dts --watch & node --watch dist/index.mjs",
